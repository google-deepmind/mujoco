<mujoco model="Soft body">
  <include file="scene.xml"/>

  <extension>
    <plugin plugin="mujoco.elasticity.corotational">
      <instance name="elasticity">
        <config key="modulus" value="1e7"/>
      </instance>
    </plugin>
  </extension>

  <compiler autolimits="true"/>

  <size memory="2M"/>

  <worldbody>
    <body pos="-.5 -.5 1">
      <freejoint/>
      <geom size=".1" density="10" pos=".5 .5 .5" group="4"/>
      <body pos="0 0 0">
        <joint type="slide" axis="1 1 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v0"/>
      </body>
      <body pos="1 0 0">
        <joint type="slide" axis="-1 1 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v1"/>
      </body>
      <body pos="1 1 0">
        <joint type="slide" axis="1 0 0"/>
        <joint type="slide" axis="0 1 0"/>
        <joint type="slide" axis="0 0 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v2"/>
      </body>
      <body pos="0 1 0">
        <joint type="slide" axis="1 0 0"/>
        <joint type="slide" axis="0 1 0"/>
        <joint type="slide" axis="0 0 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v3"/>
      </body>
      <body pos="0 0 1">
        <joint type="slide" axis="1 0 0"/>
        <joint type="slide" axis="0 1 0"/>
        <joint type="slide" axis="0 0 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v4"/>
      </body>
      <body pos="1 0 1">
        <joint type="slide" axis="1 0 0"/>
        <joint type="slide" axis="0 1 0"/>
        <joint type="slide" axis="0 0 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v5"/>
      </body>
      <body pos="1 1 1">
        <joint type="slide" axis="1 1 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v6"/>
      </body>
      <body pos="0 1 1">
        <joint type="slide" axis="-1 1 1"/>
        <geom size=".1" density="10"/>
        <plugin instance="elasticity"/>
        <site name="v7"/>
      </body>
    </body>
  </worldbody>
</mujoco>
