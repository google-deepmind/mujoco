    <motor ctrlrange="-0.05 0.05"/>
    <default class="cubelet">
      <joint type="ball" armature="0.0001" damping="0.0005" frictionloss="0.0005"/>
      <geom type="mesh" condim="1" margin="-0.001" mesh="cubelet"/>
